<p id="notice"><%= notice %></p>

<h1>S3 Photos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_photos.each do |post_photo| %>
      <tr>
        <td><%= post_photo.title %></td>
        <td><%= post_photo.content %></td>
        <td><%= link_to 'Show', post_photo %></td>
        <td><%= link_to 'Edit', edit_post_photo_path(post_photo) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo', new_post_photo_path %>
